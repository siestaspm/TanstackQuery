{
  "name": "TanstackQuery",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev:debug": "npm run kill-metro && watchman watch-del-all && cross-env ENVFILE=.env.development react-native run-android --mode=debug",
    "android:prod:debug": "npm run kill-metro && watchman watch-del-all && cross-env ENVFILE=.env.production react-native run-android --mode=debug",
    "android:dev:release": "npm run kill-metro && watchman watch-del-all && cd android && ENVFILE=.env.development ./gradlew :app:assembleRelease",
    "android:prod:release": "npm run kill-metro && watchman watch-del-all && cd android && ENVFILE=.env.production ./gradlew :app:assembleRelease",
    "ios:dev:debug": "cross-env ENVFILE=.env.development react-native run-ios --mode Debug",
    "ios:prod:debug": "cross-env ENVFILE=.env.production react-native run-ios --mode Debug",
    "ios:dev:release": "cross-env ENVFILE=.env.development react-native run-ios --mode Release",
    "ios:prod:release": "cross-env ENVFILE=.env.production react-native run-ios --mode Release",
    "kill-metro": "lsof -ti :8081 | xargs kill -9 || echo 'no metro running'",
    "build:android:prod": "NODE_ENV=production cd android && ./gradlew assembleRelease",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native/new-app-screen": "0.82.1",
    "@react-navigation/bottom-tabs": "^7.8.11",
    "@react-navigation/native": "^7.1.24",
    "@react-navigation/native-stack": "^7.8.5",
    "@rraut/react-native-faster-image": "^1.0.2",
    "@tanstack/query-sync-storage-persister": "^5.90.13",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-persist-client": "^5.90.13",
    "axios": "^1.13.2",
    "expo-device": "^8.0.9",
    "lodash": "^4.17.21",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-config": "^1.6.1",
    "react-native-dotenv": "^3.4.11",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-image-crop-picker": "^0.51.1",
    "react-native-image-picker": "^8.2.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.3.3",
    "react-native-nitro-modules": "^0.31.10",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-svg": "^15.15.1",
    "react-native-svg-transformer": "^1.5.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "0.82.1",
    "@react-native/metro-config": "0.82.1",
    "@react-native/typescript-config": "0.82.1",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.1",
    "@types/react-test-renderer": "^19.1.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-query-external-sync": "^2.2.3",
    "react-test-renderer": "19.1.1",
    "socket.io-client": "^4.8.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
